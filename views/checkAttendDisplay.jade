extends layout
block headExtra
  link(href='/css/checkAttendDisplay.css', rel='stylesheet')
block content
  .container-fluid
    // Page Heading
    .row
      .col-lg-12
        h1.page-header
          | 출석 체크
          small  Check Attendance
    p.cRate 집중도 : #{params.concentRate} 
    if params.users
      each user in params.users
        .row
          .userDiv.col-xs-6.col-lg-8.col-lg-offset-2
            img.userPic(src='#{user.imgSrc}')
          .userDiv.col-xs-6.col-lg-8.col-lg-offset-2
            .table-responsive
              table.table.table-hover.table-striped.att
                tr
                  th 학번
                tr
                  td #{user.id}
                tr
                  th 이름
                tr
                  td #{user.name}
  script(src='/js/register.js')
